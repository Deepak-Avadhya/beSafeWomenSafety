<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.deepakavadhya.www.myapplication"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.deepakavadhya.www.myapplication.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.deepakavadhya.www.myapplication.SendLocationToContact" />
        <activity android:name="com.deepakavadhya.www.myapplication.RegisterContactMenu" />
        <activity android:name="com.deepakavadhya.www.myapplication.RegisterYourselfMenu" />
        <activity android:name="com.deepakavadhya.www.myapplication.CallContact" />
        <activity android:name="com.deepakavadhya.www.myapplication.ShowPath" />
        <activity android:name="com.deepakavadhya.www.myapplication.SafePlaces" />
    </application>

</manifest>